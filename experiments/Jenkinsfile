pipeline{
    agent any
    tools{
        maven MAVEN3
    }
    stages{
        stage('get code'){
            steps{
                git branch: 'paac' , url: 'https://github.com/devopshydclub/vprofile-project.git'
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean install'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
            }
        }
    }
}